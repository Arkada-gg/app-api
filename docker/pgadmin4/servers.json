{
  "Servers": {
    "1": {
      "Name": "arkada",
      "Group": "Servers",
      "Host": "postgres",
      "Port": 5432,
      "MaintenanceDB": "$POSTGRES_DB",
      "Username": "$POSTGRES_USER",
      "PasswordExecCommand": "echo $POSTGRES_PASSWORD",
      "UseSSHTunnel": 0,
      "TunnelPort": "22",
      "TunnelAuthentication": 0,
      "KerberosAuthentication": false,
      "ConnectionParameters": {
        "sslmode": "prefer",
        "connect_timeout": 10
      }
    }
  }
}
